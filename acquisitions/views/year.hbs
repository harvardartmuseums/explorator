<div class="container w-75 mb-4 mt-4">
{{#each data}}
    <div class="row">
        <div class="col-12">
            <div class="bg-dark" style="height: 15px;"></div>
        </div> 
    </div>
    <div class="row">
        <div class="col-3">
            <h1>{{@key}}</h1>
            <h2>{{this.length}} objects</h2>
        </div>
        <div class="col-9">
            {{#each this}}    
                <a href="https://hvrd.art/o/{{id}}" target="_blank">
                {{#if colors}}
                <div style="width:100%;height:10px;background:{{makeGradient colors}};"></div>
                {{else}}
                <div style="width:100%;height:10px;background:#000000;"></div>
                {{/if}}
                {{!-- <div><img src="{{images.0.baseimageurl}}:IMAGE/full/!36,36/0/default.jpg"></div> --}}
                </a>
            {{/each}}
        </div>
    </div>
{{/each}}
</div>